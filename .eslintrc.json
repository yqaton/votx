{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "linebreak-style": ["off", "windows"],
    // "semi":["error", "never"],
    "comma-dangle": ["error", "never"],

    "no-unused-vars": ["error", { "varsIgnorePattern": "webpack" }],

    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [1]
  }
}
